script TRIGGER_BLACK_KNIGHT_4F_TREASURE {
	if (var(VAR_POKESCAPE_STORYMODE_PROGRESS) <= 191) {                        //should only trigger if done during the Fortress event, no Knights if post event?
        lockall
        getplayerxy(VAR_TEMP_1, VAR_TEMP_2)                                    //check where player is so Sir Owen walks to the correct place
        if (var(VAR_TEMP_1) == 1) {
		applymovement(OBJ_EVENT_ID_FOLLOWER, Movement_STORY_BKFORT_4F_1_right) //move Sir Owen to be beside the player
	    }
        if (var(VAR_TEMP_1) == 2) {
		applymovement(OBJ_EVENT_ID_FOLLOWER, Movement_STORY_BKFORT_4F_1_left)  //move Sir Owen to be beside the player
	    }
	    elif (var(VAR_TEMP_1) == 3) {
		applymovement(OBJ_EVENT_ID_FOLLOWER, Movement_STORY_BKFORT_4F_1_left)  //move Sir Owen to be beside the player
	    }
        applymovement(5, MOVEMENT_FACE_DOWN)
        applymovement(6, MOVEMENT_FACE_DOWN)
        waitmovement(0)
        namebox("Black Knight")
        msgbox(format("You aren't welcome here.\pWe can't let you through.\pIf you insist on trying to get past us, we'll have to battle you.\pIs that really what you want?"), MSGBOX_YESNO)
        if (var(VAR_RESULT) == TRUE)
        {
            msgbox(format("You have chosen... poorly."))
            closemessage
            hidenamebox
            playbgm(MUS_PS_ENCOUNTER_KNIGHTS, FALSE)
            trainerbattle_two_trainers(TRAINER_BLACKKNIGHT_SKOOL, msgbox(format("Skool: Maybe we were the ones who chose poorly...")), TRAINER_BLACKKNIGHT_SIFT, msgbox(format("Sift: Oh, I am definitely getting fired for this!")))
            setvar(VAR_POKESCAPE_STORYMODE_PROGRESS, 192)
            releaseall
            end    
        }
        else {
            msgbox(format("Good answer.\pNow leave us alone."))
            closemessage
            hidenamebox
            applymovement(OBJ_EVENT_ID_PLAYER, MOVEMENT_WALK_DOWN)
            applymovement(OBJ_EVENT_ID_FOLLOWER, MOVEMENT_WALK_DOWN)
            waitmovement(0)
            releaseall
            end
        }
        releaseall //try to make sure it doesn't trigger after the fight has been done
		end
    }   
}

script TRIGGER_BLACK_KNIGHT_4F_FINAL {
        lockall
        facefollower
        namebox("Sir Owen")
        msgbox(format("{PLAYER}, I can hear voices inside.\pLet's see if we can listen in and find out what they're up to without blowing our cover any further."))
        closemessage
        hidenamebox        
        applymovement(OBJ_EVENT_ID_FOLLOWER, Movement_InteractFACEUP)
        applymovement(OBJ_EVENT_ID_PLAYER, Movement_InteractFACEUP)
        waitmovement(0)
        special(SpawnCameraObject)
        applymovement(OBJ_EVENT_ID_CAMERA, movement_BK_Fort_camera_1)
        waitmovement(0)

        namebox("Daquarius")
        msgbox(format("It is as my scout said Sigmund.\pThe complete mural is right there."))
        closemessage
        hidenamebox 
        namebox("Sigmund")
        msgbox(format("That mural is of no concern.\pWhere is the artefact?"))
        closemessage
        hidenamebox 
        applymovement(1, Movement_InteractFACERIGHT)
        delay(30)
        applymovement(17, Movement_BKF_DeliverStone_1)
        delay(60)
        applymovement(1, Movement_InteractFACEDOWN)
        waitmovement(0)
        addobject(18)                                              //Adding artefact to table
        clearflag(FLAG_TEMP_F)
        applymovement(17, Movement_BKF_DeliverStone_2)
        waitmovement(0)
        namebox("Daquarius")
        msgbox(format("Here, as promised."))
        closemessage
        hidenamebox 
        applymovement(2, Common_Movement_WalkUp)
        waitmovement(0)
        namebox("Sigmund")
        msgbox(format("Good.\pYou black knights have served your purpose then."))
        closemessage
        hidenamebox 
        removeobject(18)
        setflag(FLAG_TEMP_F)
        delay(60)
        applymovement(1, MOVEMENT_INTERACT_DOWN)
        namebox("Daquarius")
        msgbox(format("…{PAUSE 15} Dont forget, this is an alliance.\pWe have equal claim to this power once you get your hands on it."))
        closemessage
        hidenamebox 
        waitmovement(0)
        //setobjectxy(3, 16,10)
        //setobjectxyperm(3, 16,10)
        applymovement(OBJ_EVENT_ID_FOLLOWER, Movement_BKF_BossEvent_1)
        applymovement(OBJ_EVENT_ID_PLAYER, Movement_BKF_BossEvent_1)
        //applymovement(3, Movement_BKF_BossEvent_1)
        namebox("Sigmund")
        msgbox(format("You will get what you deserve in due time."))
        waitmovement(0)
        applymovement(2, Movement_InteractFACELEFT)
        msgbox(format("Now…"))
        waitmovement(0)
        applymovement(2, Movement_InteractFACEUP)
        waitmovement(0)
        msgbox(format("I will take my leave, and you will deal with this pest problem right here."))
        closemessage
        hidenamebox 

        
        

        //player walks with black knight guards
        applymovement(2, Movement_InteractFACELEFT)
        waitmovement(0)

        //faceplayer
        namebox("Sigmund")
        msgbox(format("Running with your life barely intact from a volcano, then struggling your way here.\pI would call you remarkeable if you werent a monster sympathiser."))
        closemessage
        hidenamebox 
        applymovement(2, MOVEMENT_WALKLEFT)
        waitmovement(0)
        //walks up to player
        namebox("Sigmund")
        msgbox(format("These Zamorakian scum have their purpose, and right now that purpose is to end you.\pThink you can stop us?\pYou cant, now begone."))
        closemessage
        hidenamebox 
        //attempts to walk away, sir owen steps up.
        applymovement(OBJ_EVENT_ID_PLAYER, Movement_InteractFACEDOWN)
        applymovement(2, movement_WALK_DOWN_1_STEP)
        waitmovement(0)
        namebox("Sir Owen")
        msgbox(format("Stop right the-"))
        closemessage
        hidenamebox 
        applymovement(2, MOVEMENT_DraynorManorCrypt_COUNT_3)
        playse(SE_WALL_HIT)
        applymovement(OBJ_EVENT_ID_FOLLOWER, MOVEMENT_DraynorManorCrypt_HAM_3)
        waitmovement(OBJ_EVENT_ID_FOLLOWER)
        
        //pushes sir owen like vampire quest
        namebox("Sigmund")
        msgbox(format("You White Knights made your choice when you used monsters to defend yourself.\pTo call you Saradomists is a folly.\pNow leave me be and let the Zamorakian wannabe paladin up there deal with you like a nice goody two shoes."))
        closemessage
        hidenamebox 
        //walks off. says to player.
        applymovement(2, Movement_InteractFACEUP)
        waitmovement(0)
        namebox("Sigmund")
        msgbox(format("It would be mercy for you to die here.\pIf I see you meddle in our plans again.\pWell…\n{PAUSE 15}Mercy wont be on the menu."))
        closemessage
        hidenamebox 
        //walks off
        applymovement(1, Movement_BKF_BossEvent_3)
        applymovement(2, Movement_BKF_BossEvent_2)
        applymovement(4, Movement_BKF_BossEvent_5)
        waitmovement(0)
        //daquirus walks over.
        applymovement(OBJ_EVENT_ID_PLAYER, Movement_InteractFACEUP)
        applymovement(OBJ_EVENT_ID_FOLLOWER, Movement_BKF_BossEvent_4)
        namebox("Daquarius")
        msgbox(format("That guy is hard to deal with."))
        closemessage
        hidenamebox 
        waitmovement(0)
        applymovement(17, Movement_BKF_BossEvent_6)
        //faces owen
        namebox("Daquarius")
        msgbox(format("Sir Owen Sonde…{PAUSE 15}\nand a sidekick?\pOr are you his paige?\pAh but it matters not, lovely of you to join me."))
        closemessage
        hidenamebox 

        namebox("Daquarius")
        msgbox(format("Incase you a wondering, my alliance stands, and I am true to my words.\pOn behalf of the black knights, we will end you two right here.\pNow Perish!"))
        closemessage
        hidenamebox 

        trainerbattle_two_trainers(TRAINER_LORD_DAQUARIUS, msgbox(format("Daquarius: That darn Sigmund and his Saradomist ilk.\pLeaving me with the dirty work…")), TRAINER_BLACKKNIGHT_ELITE_1, msgbox(format("Elite Knight: I'm sorry sir.")))

        
        namebox("Sir Owen")
        msgbox(format("Tell us right now, where did that pink moustache twirler leave to?!"))
        closemessage
        hidenamebox 
        namebox("Daquarius")
        msgbox(format("You have the gall to ask me?\pVery well, I will tell you if you leave my fortress.\pH.A.M wants power, and there is an ancient power belonging to my lord up north.\pSigmund and his cronies wishes to unearth that power, and use it to show the world just how bad a monster can be.\pThen the world will, in his word, turn on the monsters.\pAs he uses the power of chaos itself to turn this world as he sees fit.\pI would call him mad if it did not serve my purpose.\pNow begone, get out, I have had it with you two!"))
        closemessage
        hidenamebox 
        
        setvar(VAR_POKESCAPE_STORYMODE_PROGRESS, 200)
        call(FOLLOWER_REMOVE)
        //warp outside.
        warp(MAP_ROUTE26, 24, 17)
        waitstate
        releaseall
        end
}


