script TRIGGER_BLACK_KNIGHT_4F_1 {
	if (var(VAR_POKESCAPE_STORYMODE_PROGRESS) <= 190) {                        //check storymode var, if less than post fight then enable 
        lockall
        applymovement(11, MOVEMENT_WALK_DOWN)                                  // moves Elite Knight forward one step to player and Sir Owen
        getplayerxy(VAR_TEMP_1, VAR_TEMP_2)                                    //check where player is so Sir Owen walks to the correct place
        if (var(VAR_TEMP_1) == 15) {
		applymovement(OBJ_EVENT_ID_FOLLOWER, Movement_STORY_BKFORT_4F_1_right) //move Sir Owen to be beside the player
	    }
        if (var(VAR_TEMP_1) == 16) {
		applymovement(OBJ_EVENT_ID_FOLLOWER, Movement_STORY_BKFORT_4F_1_left)  //move Sir Owen to be beside the player
	    }
	    elif (var(VAR_TEMP_1) == 17) {
		applymovement(OBJ_EVENT_ID_FOLLOWER, Movement_STORY_BKFORT_4F_1_left)  //move Sir Owen to be beside the player
	    }
        waitmovement(0)
        playse(SE_PIN)
        applymovement(9, Movement_BKF_EliteKnightSpotted)
        waitmovement(0)
        namebox("Elite Knight")
        msgbox(format("The knights on the lower floors may not be the most intelligent, but I see right through your disguises.\pThere's no need to pretend anymore, Sir Owen Sonde…"))
        closemessage
        hidenamebox
        call(CAUGHT_BY_BLACK_KNIGHTS_FINAL)
        namebox("Elite Knight")
        msgbox(format("You won't be making it past me."))
        closemessage
        hidenamebox
        namebox("Sir Owen")
        msgbox(format("For Saradomin!"))
        closemessage
        hidenamebox
        playbgm(MUS_PS_ENCOUNTER_KNIGHTS, FALSE)
        trainerbattle_no_intro(TRAINER_BLACKKNIGHT_ELITE_2, msgbox(format("…")))

        namebox("Elite Knight")
        msgbox(format("Curses on you Sir Owen Sonde, and your mangy sidekick too!"))
        closemessage
        hidenamebox
        applymovement(9, Movement_BKF_EliteKnightWalkNorth)
        waitmovement(0)

        removeobject(9)
        setflag(FLAG_TEMP_1)
        //setobjectmovementtype(9, MOVEMENT_TYPE_FACE_DOWN)
        //setobjectxy(9, 6, 11)
        //setobjectxyperm(9, 6, 11)
        facefollower
        namebox("Sir Owen")
        msgbox(format("Well so much for going icnognito.\pGuess we are going all out then?"))
        closemessage
        hidenamebox
		setvar(VAR_POKESCAPE_STORYMODE_PROGRESS, 195)
        setvar(VAR_TEMP_0, 0)
        releaseall
        end
        }
	else {                                                                     //try to make sure it doesn't trigger after the fight has already been done 
		end
		}
}


