script STORYMODE_STATE_BKFORT_4F {
    removeobject(18)
    setflag(FLAG_TEMP_F)
    removeobject(9)
    setflag(FLAG_TEMP_1)
    if (var(VAR_POKESCAPE_STORYMODE_PROGRESS) == 190) {
        addobject(9)
        clearflag(FLAG_TEMP_1)
        setvar(VAR_TEMP_0, 1)
    }
}

