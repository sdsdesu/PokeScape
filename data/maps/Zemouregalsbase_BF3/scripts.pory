script ENCOUNTER_KILLERWATT {
	lockall
	faceplayer
    namebox ("Killerwatt")
	playmoncry(SPECIES_KILLERWATT, 0)
	msgbox(format("Tzzz!"))
	waitmoncry
    hidenamebox
	closemessage
	delay(1)
	setwildbattle (SPECIES_KILLERWATT, 45, ITEM_NONE)
	dowildbattle
	setflag(FLAG_TEMP_A)
    removeobject (1)
    fadescreenswapbuffers(FADE_FROM_BLACK)
	releaseall
}




